page{
  background: #e9e8e9;
}
.container{
  width: 750rpx;
}
.header{
 
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  padding: 0 40rpx 0 30rpx;
  background: #0093d9;
}


.centent{
  display: flex;
  flex-wrap:wrap;
  padding: 0 30rpx;
  font-size: 22rpx;
}
.itme{
  width: 330rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1px solid #e9e9e9;
  /* border: 1px solid red; */
  padding: 0 5rpx;
}
.itme-right{
  margin-left: 10rpx;
}
.item-border{
  border-right: 1px solid #e9e9e9
}
.faultDescribe,.service,.contact{
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
  padding: 0 5rpx;
  border-bottom: 1px solid #e9e9e9;
}
.faultImg{
  height: 150rpx;
  width: 100%;
  border-bottom: 1px solid #e9e9e9;
  padding:30rpx 0 30rpx 10rpx;
}

.img{
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx 10rpx;
  /* border: 1px solid red; */
  display: inline-block;
}
.img image{
  width: 100rpx;
  height: 100rpx;
}

.centen-bottom{
  font-size: 20rpx;
  display: flex;
  flex-wrap:wrap;
  padding: 0 30rpx;
  margin-top: 20rpx;
}
