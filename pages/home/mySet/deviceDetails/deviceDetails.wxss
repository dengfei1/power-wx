.header{
  display: flex;
  justify-content: space-between;
  background: #fff;
}
.cancel{
  width: 150rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  justify-content: center;
  color:#333;
  font-size: 35rpx;
}
.warper{
    background: #fff;
    width: 97%;
    height: 800rpx;
    margin: 10rpx auto;
}
.img {
  width: 200rpx;
  height:200rpx;
  background: pink;
  margin: 10rpx;
}
.img image{
  width: 200rpx;
  height:200rpx;
}
.left-warper{
  padding-top: 10rpx;
}
.itme{
  margin-bottom: 10rpx;
}
.name,.tel,.address{
  display: inline-block;
  vertical-align: middle;
}
input{
  border:1px solid #ccc;
  border-radius: 10rpx;
  display: inline-block;
  width: 350rpx;
  vertical-align: middle;
  padding-left: 20rpx;
  margin-left: 10rpx;
}
.rd{
  color: red;
}
.fotn{
  font-size: 20rpx;
}
.bottom-warper{
  margin:20rpx 0 0 10rpx;
}
.bottom-warper>view{
  line-height: 45rpx;
}
/* 按钮 */
.but{
  background: #fff;
  outline: none;
  line-height: 70rpx;
  text-align: center;
  justify-content: center;
  font-size: 35rpx;
  color: #333;
}
 button::after{
    border:none;
}