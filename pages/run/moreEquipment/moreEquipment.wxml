<!-- 更多设备 -->
<view class='container '>
  <view class='tips f4'>请选择设备</view>
  <view class='list'>
      <view class='list-item f2' wx:for="{{dataList}}" wx:for-item="item"  wx:key="{{item.index}}" data-index='{{index}}' bindtap='toRun'>
          <view class='item item1'>
              <text>设备编号：{{item.uuid}}</text>
          </view>
          <view  class='item'>
              <text>机型模式：{{item.patternName}}</text>
          </view>
          <view  class='item item1'>
              <text>产品名称：{{item.name}}</text>
          </view>
          <view  class='item'>
              <text>产品机型：{{item.markerName}}</text>
          </view>
          <view  class='item'>
               <text>设备地址：{{item.clientAddress}}</text>
          </view>
         
      </view>
  </view>
</view>